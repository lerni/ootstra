{
    "name": "lerni/podcast",
    "type": "silverstripe-vendormodule",
    "description": "Silverstripe Podcast Integration",
    "keywords": [
        "Silverstripe",
        "Podcast",
        "elemental",
        "iTunes Podcasts",
        "Google Podcasts",
        "Spotify Podcasts"
    ],
    "homepage": "https://github.com/lerni/podcast",
    "license": "BSD-3-Clause",
    "authors": [{
        "name": "Lukas Erni",
        "homepage": "https://kraftausdruck.ch",
        "email": "le@kraftausdruck.ch",
        "role": "Developer"
    }],
    "require": {
        "dnadesign/silverstripe-elemental": "^4",
        "james-heinrich/getid3": "^1",
        "wilr/silverstripe-googlesitemaps": "^2"
    },
    "extra": {
        "expose": [
            "podcast/icons",
            "podcast/css"
        ]
    },
    "autoload": {
        "psr-4": {
            "App\\": "podcast/src"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
