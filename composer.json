{
	"name": "lerni/ootstra",
	"type": "silverstripe-recipe",
	"description": "kraftausdruck.ch Silverstripe CMS recipe",
	"license": "BSD-3-Clause",
	"repositories": [
		{
			"type": "vcs",
			"url": "https://github.com/lerni/silverstripe-sitetreeimporter"
		},
		{
			"type": "vcs",
			"url": "https://github.com/lerni/silverstripe-webp-image"
		},
        {
            "type": "vcs",
            "url": "git@bitbucket.org:kraftausdruck/jobpostings.git"
        },
		{
			"type": "vcs",
			"url": "git@bitbucket.org:kraftausdruck/simplebasket.git"
		},
		{
			"type": "vcs",
			"url": "git@bitbucket.org:kraftausdruck/emaildownload.git"
		}
	],
	"require": {
		"php": ">=7.1 <=8.2",
		"silverstripe/recipe-plugin": "^1",
		"silverstripe/recipe-core": "^4",
		"silverstripe/cms": "^4",
		"silverstripe/asset-admin": "^1",
		"bigfork/htmleditorsrcset": "^2",
		"bigfork/silverstripe-fail-whale": "^0",
		"firebrandhq/silverstripe-phonelink": "^2",
		"kinglozzer/metatitle": "^2",
		"symbiote/silverstripe-gridfieldextensions": "^3",
		"wilr/silverstripe-googlesitemaps": "^2",
		"jonom/silverstripe-betternavigator": "^5",
		"jonom/focuspoint": "^4",
		"silverstripe/userforms": "^5",
		"silverstripe/blog": "^3",
		"axllent/silverstripe-email-obfuscator": "^2",
		"jonom/silverstripe-text-target-length": "^2",
		"lerni/silverstripe-googleanalytics": "4.x-dev as 4.0",
		"gorriecoe/silverstripe-robots": "^1",
		"jonom/silverstripe-share-care": "^4",
		"dorsetdigital/silverstripe-canonical": "^1",
		"silverstripe/sharedraftcontent": "^2",
		"dnadesign/silverstripe-elemental": "^4",
		"dnadesign/silverstripe-elemental-userforms": "^3",
		"dnadesign/silverstripe-elemental-virtual": "^1",
		"bummzack/sortablefile": "^2.1",
		"silverstripe/redirectedurls": "^2",
		"undefinedoffset/silverstripe-nocaptcha": "^2",
		"lerni/silverstripe-webmanifest": "dev-master",
		"spatie/schema-org": "^3",
		"heyday/silverstripe-colorpalette": "^2",
		"betterbrief/silverstripe-googlemapfield": "^2",
		"giggsey/libphonenumber-for-php": "^8",
		"kinglozzer/htmleditorstylinghook": "^2",
		"jeroendesloovere/vcard": "dev-master#baff586cd57f197060bc9e0c8c49573b894e264c",
		"silverstripe/selectupload": "dev-master",
		"endroid/qr-code": "^4",
		"gorriecoe/silverstripe-link": "^1",
		"nomidi/silverstripe-webp-image": "dev-master as 1.0",
		"lerni/klaro-cookie-consent": "v2.x-dev",
		"league/commonmark": "^2",
		"eluceo/ical": "^2",
		"jonom/silverstripe-someconfig": "^1",
		"hamaka/userforms-cleanupsubmissions": "^2"
	},
	"require-dev": {
		"deployer/deployer": "^7",
		"silverstripe/sitetreeimporter": "dev-master",
		"dnadesign/silverstripe-populate": "^2"
	},
	"extra": {
		"expose": [
			"app/css",
			"app/fonts",
			"app/thirdparty",
			"themes/default/dist",
			"themes/default/thirdparty"
		],
		"project-files-installed": [
			".htaccess",
			"app/.htaccess",
			"app/_config.php",
			"app/_config/database.yml",
			"app/_config/mimevalidator.yml",
			"app/_config/mysite.yml",
			"app/src/Page.php",
			"app/src/PageController.php"
		],
		"public-files-installed": [
			".htaccess",
			"index.php",
			"install-frameworkmissing.html",
			"install.php",
			"web.config"
		],
		"resources-dir": "_resources"
	},
	"autoload": {
		"psr-4": {
			"App\\": "app/src"
		}
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"config": {
		"allow-plugins": {
			"composer/installers": true,
			"silverstripe/vendor-plugin": true,
			"silverstripe/recipe-plugin": true,
			"netresearch/composer-patches-plugin": true,
			"php-http/discovery": true
		}
	}
}
