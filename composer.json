{
	"name": "lerni/ootstra",
	"type": "silverstripe-recipe",
	"description": "kraftausdruck.ch Silverstripe CMS recipe",
	"license": "BSD-3-Clause",
	"repositories": [
		{
			"type": "vcs",
			"url": "https://github.com/lerni/silverstripe-embedfield"
		},
		{
			"type": "vcs",
			"url": "https://github.com/silverstripeltd/silverstripe-someconfig"
		},
		{
			"type": "vcs",
			"url": "https://github.com/lerni/silverstripe-share-care"
		},
		{
			"type": "vcs",
			"url": "https://github.com/lerni/silverstripe-selectupload"
		},
		{
			"type": "vcs",
			"url": "https://github.com/lerni/silverstripe-opengraph"
		}
	],
	"require": {
		"php": ">=8.0 <=8.5",
		"silverstripe/recipe-plugin": "^2",
		"silverstripe/recipe-core": "^6",
		"silverstripe/cms": "^6",
		"silverstripe/asset-admin": "^3",
		"bigfork/htmleditorsrcset": "^4",
		"bigfork/silverstripe-fail-whale": "^4",
		"firebrandhq/silverstripe-phonelink": "4.x-dev",
		"kinglozzer/metatitle": "^4",
		"symbiote/silverstripe-gridfieldextensions": "^5",
		"wilr/silverstripe-googlesitemaps": "^4",
		"jonom/silverstripe-betternavigator": "^7",
		"jonom/focuspoint": "^6",
		"silverstripe/userforms": "^7",
		"silverstripe/blog": "^5",
		"axllent/silverstripe-email-obfuscator": "^2",
		"jonom/silverstripe-text-target-length": "^2",
		"lerni/silverstripe-tracking": "6.x-dev",
		"tractorcow/silverstripe-robots": "^5",
		"tractorcow/silverstripe-opengraph": "dev-lerni-tweaks as 6.0.0",
		"jonom/silverstripe-share-care": "dev-updateMetaTags",
		"dorsetdigital/silverstripe-canonical": "^3",
		"silverstripe/sharedraftcontent": "^4",
		"dnadesign/silverstripe-elemental": "^6",
		"dnadesign/silverstripe-elemental-userforms": "^5",
		"dnadesign/silverstripe-elemental-virtual": "dev-main",
		"bummzack/sortablefile": "^3",
		"silverstripe/redirectedurls": "^4",
		"undefinedoffset/silverstripe-nocaptcha": "^3",
		"lerni/silverstripe-webmanifest": "6.x-dev",
		"spatie/schema-org": "^3",
		"heyday/silverstripe-colorpalette": "^4",
		"innoweb/silverstripe-googlemapfield": "^3",
		"giggsey/libphonenumber-for-php": "^8",
		"jeroendesloovere/vcard": "^1",
		"silverstripe/selectupload": "3.x-dev",
		"endroid/qr-code": "^6",
		"lerni/klaro-cookie-consent": "6.x-dev",
		"league/commonmark": "^2",
		"eluceo/ical": "^2",
		"jonom/silverstripe-someconfig": "dev-pulls/cms6-support",
		"lerni/folderindex": "1.x-dev",
		"simple-icons/simple-icons": "^15",
		"silverstripe/linkfield": "^5",
		"bigfork/silverstripe-vitesse": "^2.0",
		"axllent/silverstripe-scaled-uploads": "^2"
	},
	"require-dev": {
		"deployer/deployer": "^7",
		"dnadesign/silverstripe-populate": "^4",
		"pstaender/ssshell": "^0",
		"phpunit/phpunit": "^11.3",
		"friendsofphp/php-cs-fixer": "^3.92"
	},
	"extra": {
		"expose": [
			"app/css",
			"app/fonts",
			"app/images",
			"app/thirdparty",
			"themes/default/dist",
			"themes/default/thirdparty",
			"vendor/simple-icons/simple-icons/icons"
		],
		"project-files-installed": [
			".htaccess",
			"app/.htaccess",
			"app/_config.php",
			"app/_config/database.yml",
			"app/_config/mimevalidator.yml",
			"app/_config/mysite.yml",
			"app/src/Page.php",
			"app/src/PageController.php"
		],
		"public-files-installed": [
			".htaccess",
			"index.php",
			"install-frameworkmissing.html",
			"install.php",
			"web.config"
		],
		"resources-dir": "_resources"
	},
	"autoload": {
		"psr-4": {
			"App\\": "app/src"
		}
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"config": {
		"allow-plugins": {
			"composer/installers": true,
			"silverstripe/vendor-plugin": true,
			"silverstripe/recipe-plugin": true,
			"php-http/discovery": true
		}
	}
}
